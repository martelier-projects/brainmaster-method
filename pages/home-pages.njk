--- 
    pagination: 
        data: home 
        size: 1 
        alias: home 
    permalink: "home.html" 
    layout: layouts/base.njk
---

<h1>{{ home.title }}</h1>
<h1>{{ title }}</h1>

<ul>
{%- for post in collections.post -%}
  <li{% if page.url == post.url %} aria-current="page"{% endif %}>{{ post.data.title }}</li>
{%- endfor -%}
</ul>
<ul>
{%- for post in collections.post -%}
  <li><a href="{{ post.url }}">{{ post.url }}</a></li>
{%- endfor -%}
</ul>